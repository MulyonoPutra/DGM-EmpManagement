{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"linebreak-style": "off",
		"@typescript-eslint/no-empty-function": "off",
    "no-mixed-spaces-and-tabs":"off"
	}
}
